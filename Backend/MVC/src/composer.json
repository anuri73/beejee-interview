{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4",
    "doctrine/migrations": "^2.0",
    "doctrine/orm": "^2.6.2",
    "klein/klein": "^2.1",
    "symfony/yaml": "2.*",
    "tightenco/collect": "^7.9",
    "twig/twig": "^3.0"
  },
  "require-dev": {
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
