{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.2",
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/migrations": "^2.0",
    "doctrine/orm": "^2.6.2",
    "klein/klein": "^2.1",
    "rakit/validation": "^1.2",
    "symfony/config": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/form": "^5.0",
    "symfony/yaml": "^5.0",
    "tightenco/collect": "^7.9",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
